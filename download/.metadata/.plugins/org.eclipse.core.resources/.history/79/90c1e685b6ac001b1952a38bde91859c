package manager;

public class Test {
	public static void main(String[] args) {
		String code1="SH600415";
		String code2="SZ002353";
		Manager t1 = new Manager(code1);
		Manager t2 = new Manager(code2);
		t1.start();
		t2.start();
		/*
		try {
			t2.join();
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		*/
		int num1 = t1.getResult();
		//int num2 = t2.getResult();
		while(num1!=1)
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		System.out.println("线程1下载完成");
	}
}
